      "['o0(c1;c0)', 'o1(c2;c1)']\n",
      "o0 c1;c0\n",
      "o1 c2;c1\n"
     ]
    }
   ],
   "source": [
    "import re\n",
    "\n",
    "tree_str = \"o0(c1;c0),o1(c2;c1)\"\n",
    "operations = tree_str.split(\",\")\n",
    "print(operations)\n",
    "regex = r\"(o.*)\\((.*)\\)\"\n",
    "for operation in operations:\n",
    "    match = re.match(regex,operation)\n",
    "    if match:\n",
    "        node, edges = match.groups()\n",
    "        print(node, edges)\n",
    "\n"
   ]
  }
